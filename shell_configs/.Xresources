dmenu.selbackground: #85678f
!dmenu.selforeground: #2b303b

! XST Customization
! Border
st.borderpx: 2

! Opacity
!st.opacity: 255
!st.opacity: 230
urxvt*depth: 32
!urxvt*background: #1D1F21



! Font
st.font: Ubuntu Mono Nerd Font:pixelsize=15
st.chscale: 0.9

! Cursor
st.cursorshape: 4
URxvt.scrollBar: false
Urxvt.background: #1d1f21
!urxvt*transparent: true
urxvt*shading: 30

!URxvt.font: Iosevka:pixelsize=12:antialias=true:autohint=true;

dmenu.font: Iosevka Nerd Font:size=20
URxvt.font: xft:Iosevka:size=12
!st.font: xft:Iosevka:size=12
!Rxvt*font:			-*-clean-medium-r-*--15-*-*-*-*-*-*-*
!Rxvt*font:			xft:Bitstream Vera Sans Mono-10
!urxvt*font: Iosevka:pixelsize=17
!urxvt.font: xft:Terminus:pixelsize:14;
!urxvt.font: xft:bitocrafull:size=17

rofi.font:                           Iosevka Nerd Font 13
rofi.show-icons:                    true
rofi.terminal:                       xst
rofi.case-sensitive:                 false
